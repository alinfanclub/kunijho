{"ast":null,"code":"import HeaderVueSub from \"./common/HeaderVueSub.vue\";\nexport default {\n  components: {\n    HeaderVueSub\n  },\n  data() {\n    return {\n      modal: false,\n      userClick: \"\",\n      copyToast: false,\n      codeNum: [{\n        key: \"FA3987334C\"\n      }, {\n        key: \"LA7683009K\"\n      }, {\n        key: \"EK3746801L\"\n      }, {\n        key: \"AK4357929A\"\n      }, {\n        key: \"LC3631733C\"\n      }, {\n        key: \"HC3232087L\"\n      }, {\n        key: \"LC3112313D\"\n      }, {\n        key: \"LC3108061H\"\n      }, {\n        key: \"LC7238724K\"\n      }]\n    };\n  },\n  methods: {\n    copyToClipBoard() {\n      var obj = document.querySelector(\".whiteBg p\");\n      var range = document.createRange();\n      range.selectNode(obj.childNodes[0]); //텍스트 정보를 Range 객체에 저장\n      var sel = window.getSelection();\n      sel.removeAllRanges(); //기존 선택정보 삭제\n      sel.addRange(range); //텍스트 정보 선택\n      document.execCommand(\"copy\"); //복사\n      sel.removeRange(range); //선택 정보 삭제\n    },\n\n    ToastCallback() {\n      this.copyToast = true;\n      if (this.copyToast == true) {\n        setTimeout(() => {\n          this.copyToast = false;\n        }, 1000);\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AA8CA,OAAOA,YAAW,MAAO,2BAA2B;AACpD,eAAe;EACbC,UAAU,EAAE;IAAED;EAAa,CAAC;EAC5BE,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE,KAAK;MACZC,SAAS,EAAE,EAAE;MACbC,SAAS,EAAE,KAAK;MAChBC,OAAO,EAAE,CACP;QACEC,GAAG,EAAE;MACP,CAAC,EACD;QACEA,GAAG,EAAE;MACP,CAAC,EACD;QACEA,GAAG,EAAE;MACP,CAAC,EACD;QACEA,GAAG,EAAE;MACP,CAAC,EACD;QACEA,GAAG,EAAE;MACP,CAAC,EACD;QACEA,GAAG,EAAE;MACP,CAAC,EACD;QACEA,GAAG,EAAE;MACP,CAAC,EACD;QACEA,GAAG,EAAE;MACP,CAAC,EACD;QACEA,GAAG,EAAE;MACP,CAAC;IAEL,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,eAAe,GAAG;MAChB,IAAIC,GAAE,GAAIC,QAAQ,CAACC,aAAa,CAAC,YAAY,CAAC;MAC9C,IAAIC,KAAI,GAAIF,QAAQ,CAACG,WAAW,EAAE;MAClCD,KAAK,CAACE,UAAU,CAACL,GAAG,CAACM,UAAU,CAAC,CAAC,CAAC,CAAC,EAAE;MACrC,IAAIC,GAAE,GAAIC,MAAM,CAACC,YAA